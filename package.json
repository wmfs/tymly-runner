{
  "name": "@wmfs/tymly-runner",
  "version": "0.0.0-semantically-released",
  "description": "A tool for running tymlys",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/tymly-runner#readme",
  "keywords": [
    "tymly",
    "package",
    "low code",
    "nodejs",
    "javascript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/tymly-runner.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/tymly-runner/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "boom": "7.2.0",
    "config": "1.30.0",
    "debug": "3.1.0",
    "lodash": "4.17.10",
    "safe-buffer": "5.1.2",
    "@wmfs/tymly": "1.7.1"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.0.3",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "5.2.0",
    "nodemon": "1.17.5",
    "nyc": "12.0.2",
    "semantic-release": "^15.5.2",
    "standard": "11.0.1",
    "@wmfs/tymly-pg-plugin": "1.9.0"
  },
  "scripts": {
    "start": "node ./lib/index.js",
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start-nodemon": "nodemon --watch ./../../blueprints --watch ./../../../private-blueprints/blueprints -e json ./lib/index.js --legacy-watch"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {"type": "build", "scope":"deps", "release": "minor"}
      ]
    }
  }
}
